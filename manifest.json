{
    "name": "Rocket Tab",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Replaces new chrome tab with minimalistic search page",
    "icons": {
        "32": "assets/icons/app/app-icon-32.png",
        "64": "assets/icons/app/app-icon-64.png",
        "128": "assets/icons/app/app-icon-128.png",
        "256": "assets/icons/app/app-icon-256.png"
    },
    "action": {
        "default_icon": {
            "32": "assets/icons/app/app-icon-32.png"
        },
        "default_title": "Rocket Tab"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab"
    ]
}
